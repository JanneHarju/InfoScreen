    "@angular/platform-server": "^4.4.6",
    "@angular/router": "^4.4.6",
    "@angular/upgrade": "~4.4.6",
    "ng2-simple-timer":"1.2.6",
    "uuid": "3.0.1",
    "node-uuid":"1.4.7",
    "core-js": "2.4.1",
    "reflect-metadata": "0.1.9",
